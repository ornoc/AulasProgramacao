package exercicios;


import java.util.Scanner;


public class Ex5 {


   public static void main (String[] args){
       Scanner ler = new Scanner(System.in);
       System.out.println("Digite um número: ");
       int numero = ler.nextInt();


       if(numero>=10 && numero<=15){
           System.out.println("O numero digitado está entre 10 e 15. Sendo ele: " +numero);
       }
       else
           System.out.println("O número digitado não esta entre 10 e 15. Sendo ele: "+numero);
   }
       }
